<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Genres</title>
</head>
<body>
  <h1>Genres</h1>
  <% genres.forEach(genre => { %>
  <h2><%= genre.genre %></h2>
  <ul>
    <% genre.songs.forEach(song => { %>
      <li><%= song %></li>
    <% }) %>
  </ul>
<% }); %>
<button onclick="window.location.href='/'">Back to Home</button>
</body>
</html>